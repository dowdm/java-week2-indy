{{#partial "content"}}
<h1>Team:</h1>
<p class="details">Team Name: {{ team.teamName }}</p>
<p class="details">Team Description: {{ team.teamDescription }}</p>
<p class="details">Team Members: </p>
    {{#each members}}
        <p>{{name}}</p>
    {{/each}}

<a href="/teams/{{team.id}}/update">Edit team</a>
<a href="/teams/{{team.id}}/delete">Delete this team</a>
<a href="/teams/{{team.id}}/members/delete">Remove members</a>


{{/partial}}

{{> layout.hbs}}